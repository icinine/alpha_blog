<h1 align="center">All Alpha Bloggers</h1>
<Div align="center">
    <%= will_paginate %>
<% @users.each do |use| %>
    <ul class = "listing">
        <div class = "row">
            <div class="well col-md-4 col-md-offset-4">
                <li><%= link_to gravatar_for(use), user_path(use) %></li>
                <li class = "article-title"> <%= link_to use.username, user_path(use) %></li>
                <li><small><%= pluralize(use.articles.count, "article") if use.articles %></small></li>
            </div>
            <%= will_paginate %>
        </div>
        
    </ul>


<% end %>

</Div>
